{
    "manifest_version": 3,
    "name": "Break",
    "version":"1.0",
    "description": "Reminder to take a break.",
    "permissions":["storage","alarms","notifications"],
    "background":{
        "service_worker": "background.js"
    },
    "action":{
        "default_popup":"popup.html",
        "default_icon":"icon.jpeg"
    },
    "content_scripts":[],
    "host_permissions":["<all_urls>"]
}